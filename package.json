{
  "name": "wisboo-sortable",
  "version": "1.0.0",
  "description": "Customized library for Wisboo FrontEnd. Original libray Angular (legacy) directive for SortableJS.",
  "main": "angular-legacy-sortable.js",
  "scripts": {
    "build": "rollup -c rollup.prod.js",
    "serve:example": "node example/app.js",
    "webdriver": "npm run webdriver:setup && npm run webdriver:start",
    "webdriver:setup": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "test:e2e": "./node_modules/.bin/protractor e2e/conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Wisboo/angular-legacy-sortablejs.git"
  },
  "keywords": [
    "angular",
    "sortable"
  ],
  "author": "V. Maldonado <vmaldonado@wisboo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Wisboo/angular-legacy-sortablejs/issues"
  },
  "homepage": "https://github.com/Wisboo/angular-legacy-sortablejs#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "eslint": "4.0.0-alpha.0",
    "express": "^4.15.3",
    "express-static": "^1.1.0",
    "protractor": "^5.1.2",
    "rollup": "^1.21.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "dependencies": {
    "sortablejs": "^1.10.0"
  }
}
